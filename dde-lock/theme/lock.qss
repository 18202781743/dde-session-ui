QFrame#passwdEnterFrame {
    border: 1px solid  rgba(255, 255, 255, 60);
    border-radius: 4px;
    background-color: rgba(255, 255, 255, 30);
}
QPushButton#KeyBoardLayoutButton {
    border-image: url(":img/keyboard.png");
    background-color: transparent;
    border-none;
    margin:0 0 0 3px;
}
QLineEdit#passwdLineEdit {
    color:white;
    font-size:24px;
    border: 1px solid  transparent;
    border-radius: 4px;
    background: transparent;
    selection-background-color: transparent;
}

QPushButton#passwdButtonIcon {
    border:none;
}
QPushButton#LockIcon {
    border-image:url(":img/unlock_normal.png");
    border:none;
}
QPushButton#LockIcon:pressed {
    border-image:url(":img/unlock_press.png");
    border:none;
}

QFrame#KeyboardLayoutFrame {
    background-color: rgba(255, 255, 255, 30);
    border: 1px solid rgba(255, 255, 255, 70);
    border-radius: 4px;
}

QFrame#LayoutBorderFrame {
    background-color: rgba(255, 0, 0, 0);
    border: none;
}

QPushButton#LayoutButton {
    background-color: rgba(255, 255, 255, 0);
    border: none;
}

QPushButton#LayoutButton:hover {
    background-color: rgba(0, 0, 0, .2);
    border: none;
}
