QPushButton#ShutDownButton {
    border-image: url(:img/shutdown_normal.png);
    background-color: transparent;
    background-repeat:no-repeat;
}
QPushButton#ShutDownButton:hover {
    border-image: url(:img/shutdown_hover.png);
    background-color: transparent ;
    background-repeat:no-repeat;
}
QPushButton#ShutDownButton:checked {
    border-image: url(:img/shutdown_press.png);
    background-color: transparent;
    background-repeat:no-repeat;
}
